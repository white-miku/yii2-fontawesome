{
  "name": "white-miku/yii2-fontawesome",
  "description": "FontAwesome 4 for Yii2",
  "keywords": [
    "yii",
    "font",
    "awesome",
    "asset",
    "bundle"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "White Miku",
      "email": "mi@white-miku.me"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "*",
    "bower-asset/fontawesome": "~4.7"
  },
  "autoload": {
    "psr-4": {
      "whitemiku\\fontawesome\\": ""
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
